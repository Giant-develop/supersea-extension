{
  "assetInfo": {
    "grid": {
      "node": {
        "selector": "article.AssetSearchList--asset",
        "injectionMethod": "append"
      },
      "link": {
        "selector": ".Asset--anchor",
        "hierarchy": "either"
      },
      "collectionLink": {
        "selector": ".AssetCardFooter--collection-name",
        "hierarchy": "either"
      },
      "image": {
        "selector": ".Image--image, .SuperSea__Image",
        "hierarchy": "child"
      }
    },
    "list": {
      "node": {
        "selector": "div[role=\"listitem\"] .AssetCell--container",
        "injectionMethod": "prepend"
      },
      "link": {
        "selector": ".AssetCell--link",
        "hierarchy": "either"
      },
      "collectionLink": {
        "selector": ".CollectionLink--link",
        "hierarchy": "either"
      },
      "image": {
        "selector": ".Image--image",
        "hierarchy": "child"
      }
    },
    "item": {
      "node": {
        "selector": ".item--summary > article, .item--small > article",
        "injectionMethod": "append"
      },
      "link": {
        "selector": ".AssetCell--link",
        "hierarchy": "child"
      },
      "collectionLink": {
        "selector": ".CollectionLink--link",
        "hierarchy": "outside"
      },
      "image": {
        "selector": ".Image--image",
        "hierarchy": "child"
      }
    }
  },
  "bundleVerification": {
    "frameSelector": ".Bundle--summary-frame",
    "linkSelector": ".Bundle--items-list > a",
    "headerSelector": "header"
  },
  "profileSummary": {
    "accountTitleSelector": ".AccountHeader--title",
    "accountEnsNameSelector": ".AccountHeader--name"
  },
  "searchResults": {
    "menuSelector": "*[class*=\"collection__DivContainer\"] ul[class*=\"Menureact__MenuBase\"]",
    "containerSelector": ".AssetSearchView--main, .collection--ActivityTabSection",
    "route": {
      "url": "/collection?collectionSlug=[COLLECTION_SLUG]&tab=supersea",
      "as": "/collection/[COLLECTION_SLUG]?tab=supersea"
    }
  }
}
